    "suncalc": "^1.8.0",
    "xml-js": "^1.6.8"
  },
  "devDependencies": {
    "hap-nodejs": "^0.4.47"
  },
  "optionalDependencies": {
    "noble": "git+https://github.com/noble/noble.git#pull/840/head"
  },
